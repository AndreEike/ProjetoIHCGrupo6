
@{
    ViewBag.Title = "Cadastro";
}


@{ if (TempData["InsertEmpresa"] != null)
    {
        <br />
        <div id="divmsg" onclick="divhidden()" style="color:green">
            <b>Empresa cadastrada com sucesso!<span style="float:right" onmouseover="this.style.cursor='pointer'">X</span></b>
        </div>
    }
    if (TempData["InsertEmpresa_"] != null)
    {
        <br />
        <div id="divmsg" onclick="divhidden()" style="color:red">
            <b>Não foi possível cadastrar empresa!<span style="float:right" onmouseover="this.style.cursor='pointer'">X</span></b>
        </div>
    }

}

<h2>Cadastro de Empresa</h2>
